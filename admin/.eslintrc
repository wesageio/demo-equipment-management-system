{
    "env": {
        "browser": true,
        "es6": true
    },
    "plugins": [
        "import"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "no-undef": [0],
        "no-console": [0],
        "no-unused-vars": "warn",
        "react/prop-types": [0],
        "camelcase": "off",
        "indent": ["warn",4, { "SwitchCase": 1 }],
        "no-eval": "error",
        "no-prototype-builtins": [0],
        "import/first": "error"
    }
}